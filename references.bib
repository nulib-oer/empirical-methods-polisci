@article{keyYouResearchGirl2019,
  title = {You {{Research Like}} a {{Girl}}: {{Gendered Research Agendas}} and {{Their Implications}}},
  issn = {1049-0965, 1537-5935},
  shorttitle = {You {{Research Like}} a {{Girl}}},
  abstract = {Political science, like many disciplines, has a ``leaky-pipeline'' problem. Women are more likely to leave the profession than men. Those who stay are promoted at lower rates. Recent work has pointed toward a likely culprit: women are less likely to submit work to journals. Why? One answer is that women do not believe their work will be published. This article asks whether women systematically study different topics than men and whether these topics may be less likely to appear in top political science journals. To answer this question, we analyzed the content of dissertation abstracts. We found evidence that some topics are indeed gendered. We also found differences in the representation of ``women's'' and ``men's'' topics in the pages of the top journals. This suggests that research agendas may indeed be gendered and that variation in research topic might be to blame for the submission gap.},
  language = {en},
  journal = {PS: Political Science \& Politics},
  doi = {10.1017/S1049096519000945},
  author = {Key, Ellen M. and Sumner, Jane Lawrence},
